{
  "name": "skatehubba",
  "version": "1.0.0",
  "description": "SkateHubba landing page and API server",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node dist/server.js",
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/public",
    "build:server": "esbuild server/index.ts --bundle --platform=node --outfile=dist/server.js --external:@neondatabase/serverless --external:pg --external:ws --format=esm",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "check": "tsc --noEmit"
  },
  "keywords": [
    "skatehubba",
    "skateboarding",
    "landing-page",
    "express",
    "drizzle"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
